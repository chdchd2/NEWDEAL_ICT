<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.TestMapper">
	
	<insert id = "write" parameterType ="testvo">
		insert into test values(seq_num.nextval,#{name})
	</insert>
	
	<select id="list" resultType="testvo">
		select * from test
	</select>











